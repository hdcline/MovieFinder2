     <form onSubmit={this.handleSubmit} className=\"form-inline my-4\">\n              <input\n                type=\"text\"\n                className=\"form-control mr-sm-2\"\n                placeholder=\"frozen\"\n                value={searchTerm}\n                onChange={this.handleChange}\n              />\n              <button type=\"submit\" className=\"btn btn-primary\">Submit</button>\n            </form>\n            {(() => {\n              if (error) {\n                return error;\n              }\n              return results.map((movie) => {\n                return <Movie key={movie.imdbID} movie={movie} />;\n              })\n            })()}\n          </div>\n        </div>\n      </div>\n    )\n  }\n}\nexport default MovieFinder;\n","/Users/huntercline/Documents/Webdesignium/movie-finder-2/src/Movie.js",["32"],"import React from 'react';\nimport { json, checkStatus } from './utils';\nclass Movie extends React.Component {\n\n  constructor(props) {\n    super(props);\n    this.state = {\n      movie: null,\n    }\n  }\n  \n  componentDidMount () {\n    fetch(`https://www.omdbapi.com/?i=${this.props.match.params.id}&apikey=b7da8d63`)\n      .then(checkStatus)\n      .then(json)\n      .then((data) => {\n        if (data.Response === 'False') {\n          throw new Error(data.Error);\n        }\n        if (data.Response === 'True') {\n          console.log(data);\n          this.setState({ movie: data, error: '' });\n        }\n      })\n      .catch((error) => {\n        this.setState({ error: error.message });\n        console.log(error);\n      })\n  }\n  render() {\n    if (!this.state.movie) {\n      return null;\n    }\n    const {\n      Title,\n      Year,\n      Plot,\n      Director,\n      imdbRating,\n      Poster,\n    } = this.state.movie;\n    return (\n      <div className=\"container\">\n        <div className=\"row pt-5\">\n          <div className=\"col-6\">\n            <h1>{Title}</h1>\n            <ul className=\"list-uns